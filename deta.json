{
    "name": "Basketball",
    "description": "A self-hosted Dribbble Playbook alternative", 
    "runtime": "python3.9",
    "env": [
        {
            "key": "DRIBBBLE_TOKEN",
            "description": "Generate this Token at https://basketballauth.deta.dev - READ THE DOCS!",
            "required": true
        },
        {
            "key": "APP_HOST",
            "description": "DO NOT TOUCH!",
            "value": "localhost",
            "required": true 
        },
        {
            "key": "APP_PORT",
            "description": "DO NOT TOUCH!",
            "value": "8000",
            "required": true 
        }
    ]
}

