{
    "name": "Basketball",
    "description": "A self-hosted Dribbble Playbook alternative", 
    "runtime": "python3.9",
    "env": [
        {
            "key": "DRIBBBLE_TOKEN",
            "description": "Generate this Token at https://auth.usebasketball.com - READ THE DOCS!",
            "required": true
        },
        {
            "key": "CACHE_EXPIRY",
            "description": "Expiry of cached items in seconds - READ THE DOCS!",
            "value": "3600",
            "required": true
        }
    ]
}

